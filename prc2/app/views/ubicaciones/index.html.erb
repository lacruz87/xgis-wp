<p id="notice"><%= notice %></p>

<h1>Ubicaciones</h1>

<table>
  <thead>
    <tr>
      <th>Direccion</th>
      <th>Comuna</th>
      <th>Lat</th>
      <th>Lng</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ubicaciones.each do |ubicacion| %>
      <tr>
        <td><%= ubicacion.direccion %></td>
        <td><%= ubicacion.comuna %></td>
        <td><%= ubicacion.lat %></td>
        <td><%= ubicacion.lng %></td>
        <td><%= link_to 'Show', ubicacion %></td>
        <td><%= link_to 'Edit', edit_ubicacion_path(ubicacion) %></td>
        <td><%= link_to 'Destroy', ubicacion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ubicacion', new_ubicacion_path %>
