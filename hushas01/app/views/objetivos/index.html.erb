<p id="notice"><%= notice %></p>

<h1>Objetivos</h1>

<table>
  <thead>
    <tr>
      <th>Comuna</th>
      <th>Zona prc</th>
      <th>Zona</th>
      <th>Constructibilidad</th>
      <th>Target uf m2</th>
      <th>Premio fusion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @objetivos.each do |objetivo| %>
      <tr>
        <td><%= objetivo.comuna.nombre %></td>
        <td><%= objetivo.zona_prc %></td>
        <td><%= objetivo.zona %></td>
        <td><%= objetivo.constructibilidad %></td>
        <td><%= objetivo.target_uf_m2 %></td>
        <td><%= objetivo.premio_fusion %></td>
        <td><%= link_to 'Show', objetivo %></td>
        <td><%= link_to 'Edit', edit_objetivo_path(objetivo) %></td>
        <td><%= link_to 'Destroy', objetivo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Objetivo', new_objetivo_path %>
