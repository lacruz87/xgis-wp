          sup_min=prop_casapi_info2.sup_terreno*0.95/10000
          sup_max=prop_casapi_info2.sup_terreno*1.05/10000
          url='http://ec2-52-15-240-198.us-east-2.compute.amazonaws.com:8080/servicios/general_response?radius%5Bkm%5D=0.3&sup_min%5Bha%5D='+sup_min.to_s+'&sup_max%5Bha%5D='+sup_max.to_s+'&commit=Buscar&lat='+prop_casapi_info2.lat.to_s+'&lng='+prop_casapi_info2.lng.to_s
        %>
        <td><%= link_to 'Ver GIS',url %></td>
        <td><%= prop_casapi_info2.publicacion %></td>
        <td><%= prop_casapi_info2.precio %></td>
        <td><%= prop_casapi_info2.sector %></td>
        <td><%= prop_casapi_info2.direccion %></td>
        <td><%= prop_casapi_info2.programa %></td>
        <td><%= prop_casapi_info2.superficie %></td>
        <td><%= prop_casapi_info2.descripcion %></td>
        <td><%= prop_casapi_info2.sup_construida %></td>
        <td><%= prop_casapi_info2.sup_terreno %></td>
        <td><%= prop_casapi_info2.lat %></td>
        <td><%= prop_casapi_info2.lng %></td>
        <td><%= prop_casapi_info2.zona %></td>
        <td><%= prop_casapi_info2.valor_uf %></td>
        <td><%= prop_casapi_info2.precio_uf %></td>
        <td><%= prop_casapi_info2.objetivo %></td>
        <td><%= prop_casapi_info2.estado %></td>
        <td><%= prop_casapi_info2.contacto %></td>
        <td><%= prop_casapi_info2.telefono %></td>
        <td><%= prop_casapi_info2.correo %></td>        
        <td><%= link_to 'Show', prop_casapi_info2 %></td>
        <td><%= link_to 'Edit', edit_prop_casapi_info2_path(prop_casapi_info2) %></td>
        <td><%= link_to 'Destroy', prop_casapi_info2, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>

