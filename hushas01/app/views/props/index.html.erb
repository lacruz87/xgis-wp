<div class="container">
	<p id="notice">
		<%= notice %>
	</p>

	<h1>Props</h1>
	<div class="table-responsive">
		<table class="table">
			<thead>
				<tr>
					<th>Fuente</th>
					<th>Url</th>
					<th>Codigo</th>
					<th>Operacion</th>
					<th>Tipo</th>
					<th></th>
					<th></th>
					<th></th>
				</tr>
			</thead>
			<tbody>
				<% @props.each do |prop| %>
				<tr>
					<td><%= prop.fuente %></td>
					<td><%= link_to 'Link',prop.url %></td>
					<td><%= prop.codigo %></td>
					<td><%= prop.operacion %></td>
					<td><%= prop.tipo %></td>
					<td><%= link_to 'Show', prop %></td>
					<td><%= link_to 'Edit', edit_prop_path(prop) %></td>
					<td><%= link_to 'Destroy', prop, method: :delete, data: { confirm: 'Are you sure?' } %></td>
				</tr>
				<% end %>

		</table>
	</div>
</div>
<div class="container">
	<%= link_to 'New Prop', new_prop_path %>

</div>
