<div class="container">
	<p id="notice">
		<%= notice %>
	</p>

	<h1>Direcciones</h1>

	<div class="table-responsive">
		<table class="table">
			<thead>
				<tr>
					<th>Direccion</th>
					<th>Comuna</th>
					<th>Latitude</th>
					<th>Longitude</th>
					<th>Argis json</th>
					<th>Zona</th>
					<th colspan="3"></th>
				</tr>
			</thead>

			<tbody>
				<% @direcciones.each do |direccion| %>
				<tr>
					<td><%= direccion.direccion %></td>
					<td><%= direccion.comuna.nombre %></td>
					<td><%= direccion.latitude %></td>
					<td><%= direccion.longitude %></td>
					<td><%= direccion.Argis_json %></td>
					<td><%= direccion.zona %></td>
					<td><%= link_to 'Show', direccion %></td>
					<td><%= link_to 'Edit', edit_direccion_path(direccion) %></td>
					<td><%= link_to 'Destroy', direccion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
				</tr>
				<% end %>
			</tbody>
		</table>
	</div>
	<br>

	<%= link_to 'New Direccion', new_direccion_path %>
</div>
